#include <iostream>
#include <vector>
#include <algorithm>
#include <numeric>
using namespace std;

int main() {
    int T;
    cin >> T;
    
    while (T--) {
        int N;
        cin >> N;
        vector<long long> A(N);
        for (int i = 0; i < N; i++) {
            cin >> A[i];
        }
        
        long long g = 0;
        for (int i = 1; i < N; i++) {
            g = __gcd(g, A[i] - A[0]);
        }
        
        long long M = (A[N-1] - A[0]) / g + 1;
        
        cout << M - N << endl;
    }
    
    return 0;
}
